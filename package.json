{
  "name": "begin-app",
  "version": "0.0.0",
  "description": "Begin basic Svelte app + API",
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "run-p start autobuild",
    "lint": "eslint src --fix",
    "start": "run-s build sandbox",
    "sandbox": "npx sandbox",
    "start:sandbox": "cross-env NODE_ENV=testing ARC_LOCAL=1 npx sandbox",
    "test": "cross-env NODE_ENV=testing PORT=6666 tape test/*-test.js ./test/**/*-test.js | tap-spec",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@architect/sandbox": "latest",
    "@babel/core": "^7.10.5",
    "@begin/data": "latest",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/client-api": "^5.3.19",
    "@storybook/svelte": "^5.3.19",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-plugin-svelte3": "",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.32",
    "postcss-load-config": "^2.1.0",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "rollup": "^2.23.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-svelte": "",
    "rollup-plugin-terser": "^6.1.0",
    "svelte": "^3.24.0",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.0.8",
    "tailwindcss": "^1.5.2",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1",
    "tiny-json-http": "^7.2.0"
  },
  "dependencies": {
    "svelte-routing": "1.4.2"
  }
}
